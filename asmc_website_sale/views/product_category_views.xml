<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="product_category_form_view_inherit_asmc_website_sale" model="ir.ui.view">
            <field name="name">product.category.form.inherit.asmc.website.sale</field>
            <field name="model">product.category</field>
            <field name="inherit_id" ref="product.product_category_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="//group[@name='first']" position="after">
                    <group name="group_shippin_lead_time" string="eCommerce Lead Times">
                        <field name="lead_time"/>
                        <field name="is_shipping"/>
                    </group>
                </xpath>
            </field>
        </record>
    </data>
</odoo>