<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="product_template_only_form_view_inherit_asmc_website_sale" model="ir.ui.view">
            <field name="name">product.template.product.form.inherit.asmc.website.sale</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_only_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='website_id']" position="before">
                    <field name="is_shippable"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>